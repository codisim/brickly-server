model Review {
  id         String  @id @default(uuid())
  rating     Int?
  comment    String

  userId     String
  user       User    @relation(fields: [userId], references: [id])

  propertyId String
  property   Property @relation(fields: [propertyId], references: [id])

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
